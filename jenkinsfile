node {
      stage('Checkout SCM') {
            git branch: 'main', url: 'https://github.com/dolapojohnson/ng-fundamental.git'
      }
      
      stage('Install node modules') {
            bat "npm install"
      }
      
      stage('Test') {
            bat "npm run test-headless"
      }
      
      stage('Build') {
            bat "npm run build"
      }
}
